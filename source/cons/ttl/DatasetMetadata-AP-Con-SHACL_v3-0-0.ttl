dm:alternativeVersionAndPreferredVersion-dependency
        a               sh:PropertyShape ;
        sh:sparql       dm:alternativeVersionAndPreferredVersion-dependencySparql ;
        sh:description  "This constraint validates if either of the properties dcatcim:alternativeVersionOf or dcatcim:preferredVersion is present, the other one is required. Either the two properties shall be present or none." ;
        sh:name         "alternativeVersionAndPreferredVersion-dependency" ;
        sh:path         rdf:type;
        sh:group        dm:CardinalityGroupCustom ;
        sh:order        17 ;
        sh:severity     sh:Violation .
        
    
dm:alternativeVersionAndPreferredVersion-dependencySparql
		a         sh:SPARQLConstraint ;  
		sh:message "One of the properties dcatcim:alternativeVersionOf or dcatcim:preferredVersion is not present." ;
		sh:prefixes cim: ;
		sh:select """
	      SELECT $this ?value 
        WHERE {
		BIND(EXISTS{$this dcatcim:alternativeVersionOf ?value} AS ?hasaltv).
		BIND(EXISTS{$this dcatcim:preferredVersion ?value1} AS ?hasprefv).

        FILTER ((?hasaltv=true && ?hasprefv=false) || (?hasaltv=false && ?hasprefv=true)).
        }""" .			



dm:forwardReverse-dependency
        a               sh:PropertyShape ;
        sh:sparql       dm:forwardReverse-dependencySparql ;
        sh:description  "This constraint validates if at least one of the properties dcatcim:forwardDifference, dcatcim:referseDifference is present." ;
        sh:name         "forwardReverse-dependency" ;
        sh:path         rdf:type;
        sh:group        dm:CardinalityGroupCustom ;
        sh:order        18 ;
        sh:severity     sh:Violation .
        
    
dm:forwardReverse-dependencySparql
		a         sh:SPARQLConstraint ;  
		sh:message "None of the properties dcatcim:forwardDifference, dcatcim:referseDifference is present." ;
		sh:prefixes cim: ;
		sh:select """
	      SELECT $this ?value 
        WHERE {
		BIND(EXISTS{$this dcatcim:forwardDifference ?value} AS ?hasaltv).
		BIND(EXISTS{$this dcatcim:referseDifference ?value1} AS ?hasprefv).

        FILTER ((?hasaltv=false && ?hasprefv=false) ).
        }""" .
		
dm:reverseDifferencePreviousVersion-dependency
        a               sh:PropertyShape ;
        sh:sparql       dm:reverseDifferencePreviousVersion-dependencySparql ;
        sh:description  "If dcatcim:reverseDifference exists there must be a dcat:previousVersion." ;
        sh:name         "reverseDifferencePreviousVersion-dependency" ;
        sh:path         rdf:type;
        sh:group        dm:CardinalityGroupCustom ;
        sh:order        19 ;
        sh:severity     sh:Violation .
        
    
dm:reverseDifferencePreviousVersion-dependencySparql
		a         sh:SPARQLConstraint ;  
		sh:message "dcatcim:reverseDifference exists but dcat:previousVersion is not present." ;
		sh:prefixes cim: ;
		sh:select """
	      SELECT $this ?value 
        WHERE {
		BIND(EXISTS{$this dcatcim:reverseDifference ?value} AS ?hasrd).
		BIND(EXISTS{$this dcat:previousVersion ?value1} AS ?haspv).

        FILTER (?hasrd=true && ?haspv=false).
        }""" .			
